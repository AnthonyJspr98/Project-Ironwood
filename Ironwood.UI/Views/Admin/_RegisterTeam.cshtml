@using Ironwood.Application.Teams.Commands
@model RegisterTeamCommand


<form asp-action="RegisterTeam" asp-controller="Admin" data-ajax-success="OnSuccess">
    <div class="text-center">
        <h4>Register Team</h4>
    </div>
    <div class="form-group">
        <label asp-for="TeamName">Team Name</label>
        <input asp-for="TeamName"  class="form-control" placeholder="Team Name *" required="">
    </div>
    <div class="form-group">
        <label asp-for="Country">Country</label>
        <input asp-for="Country" class="form-control" placeholder="Country *" required="">
    </div>
    <div class="form-group text-center">
        <button class="btn btn-outline-primary mt-3 form-control" type="submit">SUBMIT</button>
    </div>
</form>
